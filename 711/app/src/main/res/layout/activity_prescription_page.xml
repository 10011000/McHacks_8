<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PrescriptionPage"
    android:background="@drawable/gradient_background"
    >

    <Button
        android:id="@+id/PrescriptionPageGoBack"
        android:layout_width="102dp"
        android:layout_height="55dp"
        android:background="#FFFFFF"
        android:text="Go back"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.89" />

    <TextView
        android:id="@+id/PrescriptionPageTitle"
        android:layout_width="303dp"
        android:layout_height="58dp"
        android:text="Prescription Details"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="#03A9F4"
        android:textColorLink="#03A9F4"
        android:textCursorDrawable="@android:color/holo_blue_dark"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/PrescriptionPageGoBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.297" />

    <TextView
        android:id="@+id/PrescriptionPageDetails"
        android:layout_width="253dp"
        android:layout_height="340dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/PrescriptionPageGoBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PrescriptionPageTitle"
        app:layout_constraintVertical_bias="0.569" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="195dp"
        android:layout_height="124dp"
        app:layout_constraintBottom_toTopOf="@+id/PrescriptionPageTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/syrup" />
</androidx.constraintlayout.widget.ConstraintLayout>